{
  "expo": {
    "scheme": "qrdocent",
    "name": "QR DOCENT",
    "slug": "qr-docent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#282B34"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danteedmonson.qrdocent",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes next to exhibits."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#282B34"
      },
      "package": "com.danteedmonson.qrdocent"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "description": ""
  }
}
